<Window x:Class="Final_Fantasy_XIV_Duty_Roulette.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_Fantasy_XIV_Duty_Roulette"
        mc:Ignorable="d"
        Title="FFXIV Duty Roulette" Height="450" Width="800">
    <Grid Background="WhiteSmoke">
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="0.35*"/>

        </Grid.RowDefinitions>
        <Frame x:Name="MainFrame" Grid.Column="1" Grid.Row="0" Grid.RowSpan="2"/>
        <Button x:Name="DutyRoulette" Content="Duty Roulette" Grid.Row="1" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Duty_Click" FontSize="25"/>
        <Button x:Name="ClassRoulette" Content="Class Roulette" Grid.Row="2" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" Click="Class_Click" FontSize="25"/>
    </Grid>
</Window>
